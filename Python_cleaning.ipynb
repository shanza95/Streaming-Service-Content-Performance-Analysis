   "source": [
    "# pymysql library is required to connect to MySQL using SQLAlchemy engine string like:\n",
    "\n",
    "from sqlalchemy import create_engine\n",
    "import pymysql\n",
    "\n",
    "engine = create_engine('mysql+pymysql://root:root@127.0.0.1/streaming')\n",
    "\n",
    "# use to_sql() function to write pandas DataFrame to the MySQL database\n",
    "content_library.to_sql('content_library', con=engine, if_exists='replace', index=False)\n",
    "subscription_data.to_sql('subscription_data', con=engine, if_exists='replace', index=False)\n",
    "user_viewing_data.to_sql('user_viewing_data', con=engine, if_exists='replace', index=False)"
   ]
